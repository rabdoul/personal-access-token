    "prettier": "2.1.1",
    "react-intl-properties": "0.0.1",
    "react-test-renderer": "16.13.1",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "gen-i18n": "npx react-intl-properties ./i18n/properties ./src/generated",
    "start": "npm run gen-i18n && sh generate-token.sh && react-scripts start",
    "build": "npm run gen-i18n && react-scripts build",
    "test": "npm run gen-i18n && react-scripts test",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }

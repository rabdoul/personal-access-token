    "cutting-room-notifier-client": "0.0.2",
    "lectra-auth-js": "3.2.1",
    "lectra-locale": "0.0.2",
    "node-sass": "4.14.1",
    "query-string": "6.13.1",
    "raspberry-fetch": "0.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "3.11.0",
    "react-query": "2.22.0",
    "react-scripts": "3.4.3",
    "styled-components": "5.1.1"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "3.4.1",
    "@types/jest": "^24.0.0",
    "@types/node": "12.12.55",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "5.1.3",
    "husky": "4.3.0",
    "lectra-auth-token": "0.0.1",
    "lint-staged": "10.3.0",

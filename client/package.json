{
  "name": "cutting-room-production-process",
  "version": "0.1.0",
  "proxy": "http://localhost:8080",
  "private": true,
  "dependencies": {
    "@lectra/basicbutton": "1.0.5",
    "@lectra/checkbox": "1.5.2",
    "@lectra/dropdownsearch": "2.0.3",
    "@lectra/embed-ribbon": "1.0.1",
    "@lectra/icon": "2.0.0",
    "@lectra/iconbutton": "1.0.7",
    "@lectra/input": "1.4.5",
    "@lectra/itemsswitcher": "1.2.3",
    "@lectra/ld-react-feature-flags": "1.0.39",
    "@lectra/loader": "1.0.7",
    "@lectra/select": "2.2.2",
    "@lectra/spantooltip": "1.0.6",
    "@lectra/themes": "1.1.2",
    "@lectra/tooltip": "1.0.7",
    "cutting-room-notifier-client": "0.0.2",
    "immer": "7.0.9",
    "lectra-auth-js": "3.2.1",
    "lectra-locale": "0.2.0",
    "query-string": "6.13.1",
    "raspberry-fetch": "0.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "3.11.0",
    "react-query": "2.22.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "styled-components": "5.1.1"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "3.4.1",
    "@types/jest": "25.2.3",
    "@types/node": "12.12.55",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "5.1.5",
    "@types/styled-components": "5.1.3",
    "husky": "4.3.0",
    "lectra-auth-token": "0.0.1",
    "lint-staged": "10.3.0",
    "prettier": "2.1.1",
    "react-intl-properties": "0.2.0",
    "react-test-renderer": "16.13.1",
    "source-map-explorer": "2.5.0",
    "ts-jest": "^25.5.0",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "gen-i18n": "npx react-intl-properties --src ./i18n/properties --dest ./src/generated/i18n.json",
    "start": "npm run gen-i18n && sh generate-token.sh && react-scripts start",
    "start-tmp": "npm run gen-i18n && react-scripts start",
    "build": "npm run gen-i18n && react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "npm run gen-i18n && react-scripts test",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

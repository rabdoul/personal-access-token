{
  "name": "cutting-room-production-process-client",
  "version": "1.0.0",
  "proxy": "http://localhost:8080",
  "private": true,
  "dependencies": {
    "@lectra/basicbutton": "1.1.0",
    "@lectra/checkbox": "1.6.0",
    "@lectra/dropdownsearch": "2.1.0",
    "@lectra/embed-ribbon": "1.0.13",
    "@lectra/icon": "2.2.1",
    "@lectra/iconbutton": "1.1.0",
    "@lectra/input": "2.1.0",
    "@lectra/itemsswitcher": "1.3.0",
    "@lectra/ld-react-feature-flags": "1.0.39",
    "@lectra/loader": "1.2.0",
    "@lectra/popup": "1.2.0",
    "@lectra/select": "2.4.1",
    "@lectra/spantooltip": "1.1.0",
    "@lectra/themes": "1.1.4",
    "@lectra/tooltip": "2.1.0",
    "cutting-room-notifier-client": "0.0.2",
    "cutting-room-units": "0.1.1",
    "immer": "9.0.5",
    "lectra-auth-js": "3.2.2",
    "lectra-locale": "0.3.0",
    "lodash": "4.17.21",
    "query-string": "6.13.7",
    "raspberry-fetch": "0.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-intl": "5.10.6",
    "react-query": "2.26.3",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "3.4.2",
    "@types/jest": "26.0.16",
    "@types/lodash": "4.14.165",
    "@types/node": "12.12.55",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.6",
    "@types/styled-components": "5.1.4",
    "husky": "4.3.0",
    "lectra-auth-token": "0.0.1",
    "lint-staged": "10.5.2",
    "prettier": "2.2.1",
    "react-intl-properties": "0.2.0",
    "react-test-renderer": "17.0.1",
    "source-map-explorer": "2.5.1",
    "ts-jest": "^26.4.4",
    "typescript": "4.1.2"
  },
  "scripts": {
    "gen-i18n": "npx react-intl-properties --src ./i18n/properties --dest ./src/generated/i18n.json",
    "start": "npm run gen-i18n && sh generate-token.sh && react-scripts start",
    "start-tmp": "npm run gen-i18n && react-scripts start",
    "build": "npm run gen-i18n && react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "npm run gen-i18n && react-scripts test",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "vsc-organize-imports": {
    "orderSpecifiers": true,
    "orderSpecifiersAsSingleLine": true,
    "baseUrl": "src",
    "emptyLinesAfterImports": 1,
    "emptyLinesBetweenFilledGroups": 1,
    "groups": [
      {
        "groups": [
          "global"
        ],
        "sortBy": "path",
        "emptyLines": true
      },
      {
        "groups": [
          "absolute"
        ],
        "sortBy": "path",
        "emptyLines": true
      },
      {
        "groups": [
          "relative"
        ],
        "sortBy": "path",
        "emptyLines": true
      },
      {
        "groups": [
          "globalDirect",
          "absoluteDirect",
          "relativeDirect"
        ],
        "sortBy": "path",
        "emptyLines": true
      }
    ]
  }
}
